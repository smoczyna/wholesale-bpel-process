    </message>
    <portType name="GetAltBookingServicePortType">
        <operation name="GetAltBookingServiceOperation">
            <input name="input" message="tns:GetAltBookingServiceOperationRequest"/>
            <output name="output" message="tns:GetAltBookingServiceOperationResponse"/>
        </operation>
    </portType>
    <plnk:partnerLinkType name="GetAltBookingService">
        <!-- A partner link type is automatically generated when a new port type is added. Partner link types are used by BPEL processes. 
        In a BPEL process, a partner link represents the interaction between the BPEL process and a partner service. Each partner link is associated with a partner link type.
